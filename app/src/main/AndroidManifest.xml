<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dpconde.taskexecutor">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.dpconde.taskexecutor.UserListApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name="com.dpconde.taskexecutor.mvp.view.checklistlist.ChecklistListActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dpconde.taskexecutor.mvp.view.tasklist.TaskListActivity"
            android:label="@string/activity_user_detail"
            android:parentActivityName="com.dpconde.taskexecutor.mvp.view.checklistlist.ChecklistListActivity">
        </activity>
    </application>

</manifest>